@using AlbumReviews.ViewModels;
@model List<AlbumViewModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
@foreach(AlbumViewModel album in Model){
    <header>
        @album.Title
    </header>
    <p>
        @album.Genre
    </p>
    <img src="@album.Cover"/>
    <p>
        @album.Artist
    </p>
    <p>
        @album.ReleaseDate
    </p>
}
